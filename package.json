{
  "name": "@netwin/angular-datetime-picker",
  "version": "18.0.0",
  "description": "Angular Date Time Picker",
  "keywords": [
    "Angular",
    "datepicker",
    "date picker",
    "timepicker",
    "time picker",
    "datetime picker",
    "date time picker",
    "material",
    "ngx"
  ],
  "author": "Maintained and updated by NetWin, forked from Daniel Moncada, original implementatiom by Daniel Pan",
  "license": "MIT",
  "scripts": {
    "start": "ng serve docs",
    "build": "npm run build:lib && npm run build:docs",
    "build:docs": "ng build docs",
    "build:lib": "ng build picker",
    "build:styles:windows": "mkdir \"dist/picker/assets/style\" && sass --style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
    "build:styles:linux": "mkdir -p dist/picker/assets/style && sass --style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
    "test": "ng test picker",
    "test:ci": "npm run test -- --watch=false",
    "lint": "ng lint",
    "version": "node ./build/version.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NetWin/date-time-picker.git"
  },
  "dependencies": {
    "@angular/animations": "^18.0.5",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "^18.0.5",
    "@angular/compiler": "^18.0.5",
    "@angular/core": "^18.0.5",
    "@angular/forms": "^18.0.6",
    "@angular/platform-browser": "^18.0.5",
    "@angular/platform-browser-dynamic": "^18.0.6",
    "@angular/router": "^18.0.6",
    "@ng-doc/app": "^17.6.14",
    "@ng-doc/builder": "^17.6.14",
    "@ng-doc/core": "^17.6.14",
    "@ng-doc/ui-kit": "^17.6.14",
    "core-js": "^3.37.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "^0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.6",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.14.10",
    "angular-eslint": "18.0.1",
    "eslint": "^9.6.0",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^18.0.0",
    "sass": "^1.77.6",
    "typescript": "~5.4.5",
    "typescript-eslint": "8.0.0-alpha.20"
  }
}
